{
  "name": "@kienngo/clipfs",
  "version": "1.0.1",
  "description": "CLI tools for IPFS",
  "type": "module",
  "bin": {
    "clipfs": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/inquirer": "^9.0.7",
    "typescript": "^5"
  },
  "dependencies": {
    "cli-table3": "^0.6.5",
    "inquirer": "^12.4.3"
  },
  "scripts": {
    "build": "tsc",
    "pintable": "command -v bun >/dev/null && bun dist/pintable.js || node dist/pintable.js",
    "peertable": "command -v bun >/dev/null && bun dist/peertable.js || node dist/peertable.js",
    "pin": "command -v bun >/dev/null && bun dist/pin.js || node dist/pin.js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kien-ngo/clipfs.git"
  },
  "author": "Kien Ngo",
  "license": "MIT",
  "keywords": [
    "ipfs",
    "ipfs-kubo",
    "ipfs-webui",
    "bun"
  ]
}